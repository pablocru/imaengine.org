---
import { getBlogCollection } from "@utils/get-static-paths-filtered";
import Post, { type Props } from "@components/Post.astro";

export async function getStaticPaths() {
  const blogEntries = await getBlogCollection();

  return blogEntries.map((post) => ({
    params: { blogPost: post.slug },
    props: { post },
  }));
}
---

<Post post={Astro.props.post} />
